{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "6.10"
        }
      }
    ],
    [
      "@babel/preset-react",
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./",
        "rootPathPrefix": "~"
      }
    ]
  ]
}